<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>IndexServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Studentservlet</servlet-name>
        <servlet-class>Controlador.Studentservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AcercadeServlet</servlet-name>
        <servlet-class>Controlador.AcercadeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GeneralServlet</servlet-name>
        <servlet-class>Controlador.GeneralServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdministradorServlet</servlet-name>
        <servlet-class>Controlador.AdministradorServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/IndexServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Studentservlet</servlet-name>
        <url-pattern>/Studentservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AcercadeServlet</servlet-name>
        <url-pattern>/AcercadeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GeneralServlet</servlet-name>
        <url-pattern>/GeneralServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdministradorServlet</servlet-name>
        <url-pattern>/AdministradorServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
